        agent any
        stages {
             stage("build") {
             steps {
             echo "welcome to the build stage"
              }
          }
         stage("test") {
          steps {
          echo "Welcome to the test stage"
          }
      }
      stage("Deploy") {
         steps {
         echo "Welcome to the Deploy stage"
    
            }
       }      
          
    }
 }
 
   
      
          
          
